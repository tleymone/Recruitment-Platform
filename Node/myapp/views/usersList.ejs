<body>
    <h1>
        <%= title %>
    </h1>
    <table border>
        <thead>
            <tr>
                <th>Id User</th>
                <th>Email</th>
                <th>Last Name</th>
                <th>First Name</th>
                <th>Password</th>
                <th>Telephone</th>
                <th>creationDate</th>
                <th>Status</th>
                <th>Role</th>
                <th>Organisation</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach((user)=> { %>
                <tr>
                    <td>
                        <%= user.id %>
                    </td>
                    <td>
                        <%= user.email %>
                    </td>
                    <td>
                        <%= user.lname %>
                    </td>
                    <td>
                        <%= user.fname %>
                    </td>
                    <td>
                        <%= user.password %>
                    </td>
                    <td>
                        <%= user.telephone %>
                    </td>
                    <td>
                        <%= user.creationDate %>
                    </td>
                    <td>
                        <%= user.status %>
                    </td>
                    <td>
                        <%= user.role %>
                    </td>
                    <td>
                        <%= user.organisation %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>